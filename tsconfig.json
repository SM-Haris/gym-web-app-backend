{
  "compilerOptions": {
    "skipLibCheck": false,
    "target": "ES2022",
    "module": "commonjs",
    "lib": ["es6", "ES2021.String"],
    "allowJs": true,
    "outDir": "build",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "inlineSources": true,
    "sourceRoot": "/"
  }
}
